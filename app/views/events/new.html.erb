<h1>Events#new</h1>
<%= form_with model: [@venue, @event] do |f| %>
    <%= f.label :title %>
    <%= f.text_field :title %>
    <br>
    <br>
    <%= f.label :date %>
    <%= f.datetime_field :date %>
    <br>
    <br>
    <%= f.label :description %>
    <%= f.text_field :description %>
    <br>
    <br>

    <%= f.collection_select(:venue_id, Venue.all, :id, :name, prompt: true) %>


    <%= f.hidden_field(:user_id, value: current_user.id) %>

    <%= f.submit "Create Event" %>
<% end %>